<div class="page-header">
  <h1>
    Users
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User ID</th>
        <th>User Name</th>
        <th>Email</th>
        <th>Created At</th>
        <th>Sign In Count</th>
        <th></th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td><%= user.sign_in_count %></td>          
        <td><a href="/users/<%= user.id %>" class="btn btn-primary">Show</a></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
